package day9;
import java.util.Random;

public class LottoNumberGen {
	public void test() {
		//Lotto 6개의 랜덤숫자.
		//1~45까지 랜덤 숫자.
		Random random = new Random();
		int[] lotto = new int[6];
		int add = 0;
		for(int i=0; i<=45; i++) {
		String sel="";
		System.out.print(i+",");
		while(true) { 
			if(i<=45) {
				System.out.println(random);
			}
		}
		
		
		}
		
		
	}
public static void main(String[] args) {
		LottoNumberGen gen = new LottoNumberGen();
					gen.test();
	}
}
