package day6;
import java.util.Scanner;
import java.util.Scanner;
public class Menu {

	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		System.out.println("[메뉴] 0.종료 1.시작");
		int select = scanner.nextInt();
		boolean flag = true;
		while(true) {
			if(select ==1) {
				System.out.println("진행");
			}else {
				System.out.println("스톱");
				flag = false;
			}
		}

	}

}
